{
    "name": "aaw-team/minipoll",
    "type": "typo3-cms-extension",
    "description": "Minipoll: a simple, lightweight poll plugin for TYPO3",
    "keywords": ["poll", "typo3"],
    "homepage": "https://github.com/aaw-team/minipoll",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Agentur am Wasser Team",
            "email": "development@agenturamwasser.ch",
            "homepage": "https://web.agenturamwasser.ch/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "typo3/cms-core": "^10.4.19",
        "paragonie/constant_time_encoding": "^2.4"
    },
    "require-dev": {
        "nimut/testing-framework": "^5.0"
    },
    "replace": {
        "typo3-ter/minipoll": "self.version"
    },
    "autoload": {
        "psr-4": {
            "AawTeam\\Minipoll\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AawTeam\\Minipoll\\Tests\\": "Tests/"
        }
    },
    "support": {
        "source": "https://github.com/aaw-team/minipoll",
        "issues": "https://github.com/aaw-team/minipoll/issues"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "typo3/cms": {
            "app-dir": ".Build",
            "web-dir": ".Build/public",
            "extension-key": "minipoll"
        }
    }
}
